{
  "name": "service-now-consumer",
  "version": "0.1",
  "type": "api",
  "docker_image": "runnerdockerrepo/service-now-consumer:0.1",
  "config_parameters": {
    "name": "aci_parameters",
    "label": "ACI Parameters",
    "order": 1,
    "controls": [
      {
        "label": "Test ACI",
        "action": "test_aci",
        "message": "Testing ACI",
        "type": "icon",
        "icon": {
          "group": "action",
          "name": "assessment"
        }
      }
    ],
    "items": [
      {
        "name": "tenant_name",
        "label": "Tenant Name",
        "type": "text",
        "environment_variable_name": "TENANT_NAME",
        "placeholder": "Tenant Name",
        "required": true,
        "order": 1,
        "verify": false
      },
      {
        "name": "ap_name",
        "label": "Application Profile Name",
        "type": "text",
        "environment_variable_name": "AP_NAME",
        "placeholder": "Application Profile Name",
        "required": true,
        "order": 2,
        "verify": false
      },
      {
        "name": "kakfa_in_topic",
        "label": "Pipeline Input topic",
        "type": "text",
        "environment_variable_name": "KAFKA_INPUT_TOPIC",
        "placeholder": "Pipeline Input Topic",
        "required": true,
        "order": 1,
        "verify": false
      },
      {
        "name": "kakfa_op_topic",
        "label": "Pipeline Output topic",
        "type": "text",
        "environment_variable_name": "KAFKA_OUTPUT_TOPIC",
        "placeholder": "Pipeline Output Topic",
        "required": true,
        "order": 2,
        "verify": false
      }
    ]
  }
}